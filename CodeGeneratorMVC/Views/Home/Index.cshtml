@model Project
@{
    ViewData["Title"] = "Home Page";
}

    <div class="text-center">
        <h1 class="display-4">Welcome</h1>
        <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
        @using (Html.BeginForm(Model.Key, "Upload", FormMethod.Post, new { enctype = "multipart/form-data" })) {
            <div>
                <label for="txtName">Your name for file comments: </label>
                <input id="txtName" name="name" />
            </div>
            <div>
                <label for="fupFile">Pick an T-SQL file to parse.</label>
                <input type="file" id="fupFile" name="fupFile" />
            </div>
            <div>
                <input type="submit" value="Upload SQL file" />
            </div>
        }
    </div>
